{
  "name": "convex-panel",
  "version": "0.1.13",
  "description": "A React component for monitoring and debugging Convex applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "style": "dist/styles/convex-panel.css",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "style": "./dist/styles/convex-panel.css"
    },
    "./utils/getConvexToken": {
      "import": "./dist/utils/getConvexToken.js",
      "require": "./dist/utils/getConvexToken.js",
      "types": "./dist/utils/getConvexToken.d.ts"
    },
    "./styles/convex-panel.css": "./dist/styles/convex-panel.css"
  },
  "files": [
    "dist",
    "examples",
    "README.md"
  ],
  "scripts": {
    "copy-src": "node copy-src.js",
    "process-tailwind": "node process-tailwind.js",
    "build": "npm run copy-src && npm run process-tailwind && rollup -c",
    "dev": "npm run copy-src && npm run process-tailwind && rollup -c -w",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "convex",
    "react",
    "debug",
    "panel",
    "monitoring"
  ],
  "author": "Robert ALvarez",
  "license": "MIT",
  "peerDependencies": {
    "convex": ">=0.9.0",
    "next": ">=13.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "debounce": "^1.2.1",
    "framer-motion": "^10.16.4",
    "lucide-react": "^0.479.0",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.3",
    "@rollup/plugin-url": "^8.0.2",
    "@types/debounce": "^1.2.1",
    "@types/node": "^22.13.10",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.4.21",
    "convex": "^1.0.0",
    "copyfiles": "^2.4.1",
    "next": "^13.4.19",
    "postcss": "^8.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.1",
    "rollup": "^3.29.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-url": "^3.0.1",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robertalv/convex-panel.git"
  },
  "bugs": {
    "url": "https://github.com/robertalv/convex-panel/issues"
  },
  "homepage": "https://github.com/robertalv/convex-panel#readme"
}
