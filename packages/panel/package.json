{
  "name": "convex-panel",
  "version": "0.2.17",
  "description": "A React component for monitoring and debugging Convex applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css",
      "default": "./dist/index.css"
    },
    "./utils/getConvexToken": {
      "types": "./dist/utils/getConvexToken.d.ts",
      "import": "./dist/utils/getConvexToken.js",
      "require": "./dist/utils/getConvexToken.js",
      "default": "./dist/utils/getConvexToken.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.esm.js",
      "require": "./dist/vite.js",
      "default": "./dist/vite.js"
    }
  },
  "files": [
    "dist",
    "examples",
    "vite.config.ts",
    "README.md"
  ],
  "scripts": {
    "copy-src": "node copy-src.js",
    "build:css": "node process-css.js",
    "build:styles-ts": "node scripts/generate-styles-ts.js",
    "build": "npm run copy-src && npm run build:css && npm run build:styles-ts && tsup",
    "dev": "tsup --watch",
    "dev:live": "vite --config dev/vite.config.ts",
    "dev:server": "node dev/server.js",
    "dev:all": "npm run dev:server & npm run dev:live",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "convex",
    "react",
    "debug",
    "panel",
    "monitoring"
  ],
  "author": "Robert Alvarez",
  "license": "MIT",
  "peerDependencies": {
    "convex": ">=0.9.0",
    "next": ">=13.0.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@monaco-editor/react": "^4.6.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/recharts": "^1.8.29",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "debounce": "^1.2.1",
    "framer-motion": "^12.5.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.479.0",
    "monaco-editor": "^0.45.0",
    "react-helmet": "^6.1.0",
    "react-hotkeys-hook": "^4.6.1",
    "react-use": "^17.6.0",
    "react-window": "^1.8.11",
    "recharts": "^2.15.1",
    "sonner": "^2.0.7",
    "swr": "^2.3.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/debounce": "^1.2.1",
    "@types/node": "^22.13.10",
    "@types/react-helmet": "^6.1.11",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.22",
    "convex": "^1.21.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "next": "^13.4.19",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^5.0.1",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robertalv/convex-panel.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/robertalv/convex-panel/issues"
  },
  "homepage": "https://convexpanel.dev"
}
