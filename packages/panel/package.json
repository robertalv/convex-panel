{
  "name": "convex-panel",
  "version": "0.2.35",
  "description": "A React component for monitoring and debugging Convex applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "sideEffects": [
    "*.css",
    "dist/**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css",
      "default": "./dist/index.css"
    },
    "./utils/getConvexToken": {
      "types": "./dist/utils/getConvexToken.d.ts",
      "import": "./dist/utils/getConvexToken.js",
      "require": "./dist/utils/getConvexToken.js",
      "default": "./dist/utils/getConvexToken.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.esm.js",
      "require": "./dist/vite.js",
      "default": "./dist/vite.js"
    },
    "./nextjs": {
      "types": "./dist/nextjs/index.d.ts",
      "import": "./dist/nextjs/index.esm.js",
      "require": "./dist/nextjs/index.js",
      "default": "./dist/nextjs/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.esm.js",
      "require": "./dist/react/index.js",
      "default": "./dist/react/index.js"
    }
  },
  "files": [
    "dist",
    "examples",
    "vite.config.ts",
    "scripts",
    "README.md"
  ],
  "scripts": {
    "copy-src": "node copy-src.js",
    "build:css": "node process-css.js",
    "build:styles-ts": "node scripts/generate-styles-ts.js",
    "build": "npm run clean && npm run copy-src && tsup && npm run build:css && npm run build:styles-ts && node scripts/print-build-size.cjs",
    "build:sequential": "node scripts/build-sequential.js",
    "dev": "tsup --watch",
    "dev:live": "vite --config dev/vite.config.ts",
    "dev:server": "node dev/server.js",
    "dev:all": "npm run dev:server & npm run dev:live",
    "setup:oauth": "node scripts/setup-oauth.js",
    "setup:env": "node scripts/setup-env.js",
    "setup": "npm run setup:oauth && npm run setup:env",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build && npm run remove-sourcemaps",
    "remove-sourcemaps": "find dist -name '*.map' -type f -delete",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "convex",
    "react",
    "debug",
    "panel",
    "monitoring"
  ],
  "author": "Robert Alvarez",
  "license": "MIT",
  "peerDependencies": {
    "classnames": "^2.5.1",
    "convex": ">=0.9.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.5.0",
    "lucide-react": "^0.479.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "react-window": "^1.8.11",
    "sonner": "^2.0.7"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "framer-motion": {
      "optional": true
    }
  },
  "dependencies": {
    "@convex-panel/shared": "workspace:*",
    "@convex-dev/agent": "^0.3.2",
    "@emotion/is-prop-valid": "^1.3.1",
    "@xterm/addon-fit": "^0.11.0",
    "@xterm/addon-web-links": "^0.12.0",
    "@xterm/xterm": "^6.0.0",
    "@xyflow/react": "^12.10.0",
    "ace-builds": "^1.43.5",
    "debounce": "^1.2.1",
    "lodash": "^4.17.21",
    "react-ace": "^14.0.1",
    "react-hotkeys-hook": "^4.6.1",
    "react-markdown": "^10.1.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window-infinite-loader": "^2.0.0",
    "recharts": "^2.15.1",
    "remark-gfm": "^4.0.1",
    "swr": "^2.3.3",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/debounce": "^1.2.1",
    "@types/node": "^22.13.10",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-helmet": "^6.1.11",
    "@types/react-window": "^1.8.8",
    "@types/recharts": "^1.8.29",
    "@vitejs/plugin-react": "^4.3.1",
    "ace-builds": "^1.43.5",
    "autoprefixer": "^10.4.22",
    "classnames": "^2.5.1",
    "convex": "^1.21.0",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "framer-motion": "^12.5.0",
    "lucide-react": "^0.479.0",
    "next": "^13.4.19",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "react": "^19.2.0",
    "react-ace": "^14.0.1",
    "react-dom": "^19.2.0",
    "react-window": "^1.8.11",
    "recharts": "^2.15.1",
    "rimraf": "^5.0.1",
    "sonner": "^2.0.7",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.8.1",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^5.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robertalv/convex-panel.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/robertalv/convex-panel/issues"
  },
  "homepage": "https://convexpanel.dev",
  "bin": {
    "convex-panel": "./bin/convex-panel.js"
  }
}
