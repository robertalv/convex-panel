{
  "name": "@convex-panel/registry",
  "version": "0.0.1",
  "description": "Convex Components Registry - Component metadata and registry API",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src/data"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "update-downloads": "tsx scripts/update-npm-downloads.ts"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0 || ^19.0.0",
    "tsup": "^8.5.1",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
